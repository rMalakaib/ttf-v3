{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "telegramHandle": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "isNotificationsMuted": {
      "type": "boolean",
      "default": false
    },
    "projects": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project.project",
      "mappedBy": "users_permissions_users"
    },
    "answer_revisions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::answer-revision.answer-revision",
      "mappedBy": "users_permissions_user"
    },
    "question_locks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::question-lock.question-lock",
      "mappedBy": "users_permissions_user"
    },
    "submissions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::submission.submission",
      "mappedBy": "users_permissions_user"
    },
    "client_documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::client-document.client-document",
      "mappedBy": "users_permissions_user"
    },
    "activity_logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activity-log.activity-log",
      "mappedBy": "users_permissions_user"
    },
    "notification_preference": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::notification-preference.notification-preference",
      "inversedBy": "users_permissions_user"
    }
  }
}
